{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ],
  "rules": {
    "test": "png",
    "loader": "file-loader",
    "options": {
      "name": "images/[name].[ext]"
    }
  }
}
